{
   "author": "NotTobi",
   "background": {
      "scripts": [ "js/background.js" ]
   },
   "content_scripts": [ {
      "js": [ "js/content.js" ],
      "matches": [ "*://*.soundcloud.com/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "js": [ "install.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   } ],
   "description": "Adds download buttons for tracks on soundcloud.com",
   "icons": {
      "128": "icons/icon-128.png",
      "48": "icons/icon-48.png",
      "64": "icons/icon-64.png",
      "96": "icons/icon-96.png"
   },
   "manifest_version": 2,
   "name": "SoundCloud Downloader v2",
   "options_ui": {
      "page": "settings.html"
   },
   "page_action": {
      "default_icon": {
         "128": "icons/icon-128.png",
         "48": "icons/icon-48.png",
         "64": "icons/icon-64.png",
         "96": "icons/icon-96.png"
      },
      "default_title": "Soundcloud Downloader",
      "show_matches": [ "*://*.soundcloud.com/*" ]
   },
   "permissions": [ "downloads", "webRequest", "storage", "webRequestBlocking", "*://*.soundcloud.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2022.06.12",
   "web_accessible_resources": [ "js/repostBlocker.js" ]
}
